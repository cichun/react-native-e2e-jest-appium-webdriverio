{
  "name": "TestForE2E",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "test:all": "E2E_DEVICE=\"*\" jest --testMatch=\"**/__tests__/**/!(*e2e.js)\"",
    "start:appium": "appium",
    "test:e2e": "jest --testMatch=\"**/__tests__/**/*e2e.js\"",
    "test:e2e:android": "E2E_DEVICE=\"android-emulator\" yarn test:e2e",
    "test:e2e:ios": "E2E_DEVICE=\"ios-emulator\" yarn test:e2e"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-native": "0.69.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "appium": "^1.22.3",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "webdriverio": "^7.21.0",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "e2e": {
    "platform": {
      "ios": {
        "platformName": "iOS",
        "platformVersion": "15.5",
        "deviceName": "iPhone 13",
        "app": "./ios/build/Build/Products/Release-iphonesimulator/TestForE2E.app",
        "automationName": "XCUITest"
      },
      "android": {
        "platformName": "Android",
        "platformVersion": "12",
        "deviceName": "Android Emulator",
        "app": "TestForE2E-debug.apk",
        "automationName": "UiAutomator2"
      }
    }
  }
}
